 ///////////////////////////////////////
//┌─────┐                             //
//│ 2 4 │2 = Shift,  4 = Shift+Level3 //
//│ 1 3 │1 = Normal, 3 = Level3       //
//└─────┘                             //
//         fælz latin colemak         ///////////////////////////////////////////////////////
//┌────┬─────┬─────┬─────┬─────┬─────┬─────┬─────┬─────┬─────┬─────┬─────┬─────┲━━━━━━━━━━┓//
//│ ~  │ Æ Ä │ Å   │ Ø Ö │ Þ Ð │ { ‹ │ } › │ ; ÷ │ =   │ +   │ /   │ (   │ )   ┃          ┃//
//│ ≈  │ æ ä │ å   │ ø ö │ þ ð │ [ « │ ] » │ : × │ @ ' │ ? ¿ │ 7 \ │ 8   │ 9   ┃ Backspace┃//
//┢━━━━┷━━┱──┴──┬──┴──┬──┴──┬──┴──┬──┴──┬──┴──┬──┴──┬──┴──┬──┴──┬──┴──┬──┴──┬──┺━━┳━━━━━━━┫//
//┃       ┃ Q   │ W   │ F   │ P   │ G   │ J   │ L Ł │ U Ù │ Y Ü │ $ £ │ %   │ &   ┃       ┃//
//┃  Tab  ┃ q   │ w   │ f   │ p   │ g ˛ │ j   │ l ł │ u ù │ y ü │ 4 ¢ │ 5 € │ 6 ^ ┃ Enter ┃//
//┣━━━━━━━┻┱────┴┬────┴┬────┴┬────┴┬────┴┬────┴┬────┴┬────┴┬────┴┬────┴┬────┴┬────┺┓      ┃//
//┃        ┃ A Á │ R   │ S   │ T   │ D   │ H Ħ │ N Ñ │ E   │ I Í │ !   │ "   │ #   ┃  ⏎   ┃//
//┃Backspce┃ a á │ r   │ s ß │ t   │ d ° │ h ħ │ n ñ │ e   │ i í │ 1 ¡ │ 2   │ 3   ┃      ┃//
//┣━━━━━━┳━┹───┬─┴───┬─┴───┬─┴───┬─┴───┬─┴───┬─┴───┬─┴───┬─┴───┬─┴───┬─┴───┲━┷━━━━━┻━━━━━━┫//
//┃      ┃ _   │ Z   │ X   │ C Š │ V Œ │ B   │ K   │ M   │ O   │ ,   │ *   ┃              ┃//
//┃ Shift┃ -   │ z   │ x   │ c š │ v œ │ b ˘ │ k ° │ m ¯ │ o   │ .   │ 0   ┃  Lvl_3_Shift ┃//
//┣━━━━━━┻┱────┴┲━━━━┷━━┱──┴─────┴─────┴─────┴─────┴─────┴─────┼─────┴┲━━━━┻┳━━━━━┳━━━━━━━┫//
//┃       ┃     ┃       ┃                                      │      ┃     ┃     ┃       ┃//
//┃ Ctrl_L┃     ┃ Alt_L ┃                 Caps                 │ Alt_R┃     ┃     ┃ Ctrl_R┃//
//┗━━━━━━━┹─────┺━━━━━━━┹──────────────────────────────────────┴──────┺━━━━━┻━━━━━┻━━━━━━━┛//
////////////////////////////////////////////////////////////////////////////////////////////

default partial alphanumeric_keys
xkb_symbols "latin" {

  key <TLDE> { [     approxeq,   asciitilde                                    ] };
  key <AE01> { [           ae,           AE,      adiaeresis,       Adiaeresis ] };
  key <AE02> { [        aring,        Aring                                    ] };
  key <AE03> { [       oslash,     Ooblique,      odiaeresis,       Odiaeresis ] };
  key <AE04> { [        thorn,        THORN,             eth,              ETH ] };
  key <AE05> { [  bracketleft,    braceleft,   guillemotleft,        0x1002039 ] };
  key <AE06> { [ bracketright,   braceright,  guillemotright,        0x100203a ] };
  key <AE07> { [        colon,    semicolon,        multiply,         division ] };
  key <AE08> { [           at,        equal,      apostrophe                   ] };
  key <AE09> { [     question,         plus,    questiondown                   ] };
  key <AE10> { [            7,        slash,       backslash                   ] };
  key <AE11> { [            8,    parenleft                                    ] };
  key <AE12> { [            9,   parenright                                    ] };
  key <BKSP> { [    BackSpace,    BackSpace                                    ] };


  key <TAB>  { [          TAB,          TAB                                    ] };
  key <AD01> { [            q,            Q                                    ] };
  key <AD02> { [            w,            W                                    ] };
  key <AD03> { [            f,            F                                    ] };
  key <AD04> { [            p,            P                                    ] };
  key <AD05> { [            g,            G,     dead_ogonek                   ] };
  key <AD06> { [            j,            J,          igrave,           Igrave ] };
  key <AD07> { [            l,            L,         lstroke,          Lstroke ] };
  key <AD08> { [            u,            U,          ugrave,           Ugrave ] };
  key <AD09> { [            y,            Y,      udiaeresis,       Udiaeresis ] };
  key <AD10> { [            4,       dollar,            cent,         sterling ] };
  key <AD11> { [            5,      percent,        EuroSign,              yen ] };
  key <AD12> { [            6,    ampersand,     asciicircum,       dead_caron ] };


  key <CAPS> { [    BackSpace,    BackSpace                                    ] };
  key <AC01> { [            a,            A,          aacute,           Aacute ] };
  key <AC02> { [            r,            R                                    ] };
  key <AC03> { [            s,            S,          ssharp                   ] };
  key <AC04> { [            t,            T                                    ] };
  key <AC05> { [            d,            D,           U00B0                   ] };
  key <AC06> { [            h,            H,         hstroke,          Hstroke ] };
  key <AC07> { [            n,            N,          ntilde,           Ntilde ] };
  key <AC08> { [            e,            E,          eacute,           Eacute ] };
  key <AC09> { [            i,            I,          iacute,           Iacute ] };
  key <AC10> { [            1,       exclam,      exclamdown                   ] };
  key <AC11> { [            2,     quotedbl                                    ] };
  key <AC12> { [            3,   numbersign                                    ] };


  key <LFSH> { [      Shift_L,      Shift_L                                    ] };
  key <LSGT> { [        minus,   underscore                                    ] };
  key <AB01> { [            z,            Z                                    ] };
  key <AB02> { [            x,            X                                    ] };
  key <AB03> { [            c,            C,          scaron,           Scaron ] };
  key <AB04> { [            v,            V,              oe,               OE ] };
  key <AB05> { [            b,            B,      dead_breve                   ] };
  key <AB06> { [            k,            K,  dead_abovering                   ] };
  key <AB07> { [            m,            M,     dead_macron                   ] };
  key <AB08> { [            o,            O                                    ] };
  key <AB09> { [       period,        comma,         greater,             less ] };
  key <AB10> { [            0,     asterisk                                    ] };
  key <RTSH> { [ISO_Level3_Shift,ISO_Level3_Shift,ISO_Level3_Shift,ISO_Level3_Shift] };

  key <PRSC> { [          bar,   dead_acute,       brokenbar,        dead_grave] };
  key <SCLK> { [dead_diaeresis,dead_circumflex,   dead_tilde                   ] };
  key <PAUS> { [     asterisk,        acute,           grave,        diaeresis ] };


  key <LCTL> { [    Control_L,    Control_L                                    ] };
  key <LWIN> { [        Super,        Super                                    ] };
  key <LALT> { [        Alt_L,        Alt_L                                    ] };
  key <SPCE> { [        space                                                  ] };
  key <RALT> { [        Alt_R,        Alt_R                                    ] };
  key <RWIN> { [        Super,        Super                                    ] };
  key <RCTL> { [    Control_R,    Control_R                                    ] };


  key <INS>  { [ Insert ] };
  key <HOME> { [ Home ] };
  key <PGUP> { [ Prior ] };

  key <DELE> { [ Delete ] };
  key <END>  { [ End ] };
  key <PGDN> { [ Next ] };


  key  <UP>  { [ Up ] };

  key <LEFT> { [ Left ] };
  key <DOWN> { [ Down ] };
  key <RGHT> { [ Right ] };
};

